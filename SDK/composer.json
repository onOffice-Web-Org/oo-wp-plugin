{
    "name": "onoffice/sdk",
    "description": "Official client to communicate with the onOffice API",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "onOffice\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\onOffice\\SDK\\": "tests"
        }
    },
    "require-dev": {
      "php": ">=7.4.0 <8.3.0",
      "phpunit/phpunit": "^6 || ^7 || ^8 || ^9",
      "friendsofphp/php-cs-fixer": "^2.16",
      "symfony/process": "^5.4.36"
    }
}
